{
    "name": "schematist",
    "version": "0.0.0",
    "description": "Data modeling",
    "keywords": [],
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/schematist",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/index.d.ts"
    ],
    "es2015": "src/index.ts",
    "main": "lib-cjs/index.js",
    "module": "lib-esm/index.js",
    "browser": "lib-esm/index.js",
    "types": "./typings/index.d.ts",
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "types": "tsc --project ./tsconfig.decl.json",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require @babel/register --recursive tests/unit",
        "prepublishOnly": "npm run build:cjs && npm run build:esm"
    },
    "devDependencies": {
        "@babel/core": "^7.7.2",
        "@babel/register": "^7.7.0",
        "@babel/cli": "^7.7.0",
        "@babel/preset-env": "^7.7.1",
        "typescript": "^3.7.2",
        "tsd": "^0.11.0",
        "mocha": "^6.2.2",
        "chai": "^4.2.0"
    },
    "dependencies": {
        "message-tag": "^0.4.2"
    }
}
